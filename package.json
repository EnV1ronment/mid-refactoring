{
  "name": "express-ts",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "eslint src --fix --ext .ts,.tsx ",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/src/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "doc": "apidoc -i src/routes/ -o src/public/apidoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "apidoc": {
    "name": "ems2.0-mm-controller-api-doc",
    "version": "0.0.0",
    "description": "ems2.0-mm-controller node api document",
    "title": "ems2.0-mm-controller node api-doc",
    "sampleUrl": "http://localhost:3001",
    "preview-url": "http://localhost:3001/apidoc"
  },
  "dependencies": {
    "apidoc": "^0.17.7",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "dotenv": "^8.1.0",
    "eureka-js-client": "^4.3.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.3.0",
    "log4js": "^5.2.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "pm2": "^3.5.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/http-errors": "^1.6.2",
    "@types/jasmine": "^3.4.4",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.12",
    "@types/request": "^2.48.3",
    "@types/supertest": "^2.0.8",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "gulp": "4.0.2",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-rename": "^1.4.0",
    "install": "^0.13.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^5.0.0",
    "mocha": "^6.1.4",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
